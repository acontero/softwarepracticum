package testForValidConvexHull;

public class Coord implements Object{
	int x;
	int y;
	
	public Coord(int first, int second){
		x = first;
		y = second;
	}
	
	@Override
	public boolean equals(Coord other){
	    if (this.x!=other.x) return false;
	    if (this.y!=other.y) return false;
	    else return true;
	}

}
